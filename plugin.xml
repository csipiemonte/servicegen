<plugin>

   <extension point="org.eclipse.emf.edit.itemProviderAdapterFactories">
      <factory
            uri="http://www.csi.it/mddtools/servicegen"
            class="it.csi.mddtools.servicegen.provider.ServicegenItemProviderAdapterFactory"
            supportedTypes=
              "org.eclipse.emf.edit.provider.IEditingDomainItemProvider
               org.eclipse.emf.edit.provider.IStructuredItemContentProvider
               org.eclipse.emf.edit.provider.ITreeItemContentProvider
               org.eclipse.emf.edit.provider.IItemLabelProvider
               org.eclipse.emf.edit.provider.IItemPropertySource"/>
   </extension>

   <extension point="org.eclipse.emf.ecore.generated_package">
      <package
            uri="http://www.csi.it/mddtools/servicegen"
            class="it.csi.mddtools.servicegen.ServicegenPackage"
            genModel="src/metamodel/servicegen_metamodel.genmodel"/>
      <package
            class="it.csi.mddtools.servicedef.ServicedefPackage"
            genModel="src/metamodel/servicedef_metamodel.genmodel"
            uri="http://www.csi.it/mddtools/servicedef">
      </package>
      <package
            class="it.csi.mddtools.appresources.AppresourcesPackage"
            genModel="src/metamodel/resources_metamodel.genmodel"
            uri="http://www.csi.it/mddtools/appresources">
      </package>
      <package
            class="it.csi.mddtools.svcorch.SvcorchPackage"
            genModel="src/metamodel/svcorch_metamodel.genmodel"
            uri="http://www.csi.it/mddtools/svcorch">
      </package>
      <package
            class="it.csi.mddtools.typedef.TypedefPackage"
            genModel="src/metamodel/typedef_metamodel.genmodel"
            uri="http://www.csi.it/mddtools/typedef">
      </package>
   </extension>
   <extension
         point="org.openarchitectureware.adapter.emf.check.checks">
      <metaModel
            nsURI="http://www.csi.it/mddtools/servicegen"
            override="false">
         <checkFile
               path="metamodel/ServicegenChecks">
         </checkFile>
      </metaModel>
      <metaModel
            nsURI="http://www.csi.it/mddtools/typedef"
            override="false">
         <checkFile
               path="metamodel/ServicegenChecks">
         </checkFile>
      </metaModel>
      <metaModel
            nsURI="http://www.csi.it/mddtools/servicedef"
            override="false">
         <checkFile
               path="metamodel/ServicegenChecks">
         </checkFile>
      </metaModel>
      <metaModel
            nsURI="http://www.csi.it/mddtools/appresources"
            override="false">
         <checkFile
               path="metamodel/ServicegenChecks">
         </checkFile>
      </metaModel>
      <metaModel
            nsURI="http://www.csi.it/mddtools/svcorch"
            override="false">
         <checkFile
               path="metamodel/ServicegenChecks">
         </checkFile>
      </metaModel>
   </extension>

</plugin>
